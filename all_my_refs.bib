@article{serrano_observacoes_2011,
  title = {Observa\c{c}\~oes {{Sobre}} as {{Teorias}} Do {{Crescimento}}},
  author = {Serrano, Franklin},
  year = {2011},
  pages = {1--23},
  abstract = {O objetivo destas notas \'e duplo. Em primeiro lugar, apresentar da forma mais simples e b\'asica poss\'ivel os aspectos principais das diversas teorias do crescimento, tanto neocl\'assicas quanto heterodoxas. Em segundo lugar, novamente considerando apenas seus aspectos mais essenciais, fazer uma avalia\c{c}\~ao cr\'itica preliminar das diversas teorias, no que diz respeito tanto a consist\^encia l\'ogico-te\'orica quanto \`a poss\'ivel relev\^ancia para o entendimento de alguns fatos estilizados b\'asicos sobre o processo de desenvolvimento. II. As teorias neocl\'assicas do crescimento II.1 O mecanismo de mercado na abordagem neocl\'assica As teorias do crescimento da abordagem neocl\'assica (ou marginalista) s\~ao baseadas na vis\~ao da opera\c{c}\~ao normal do mecanismo de mercado erigida a partir do chamado princ\'ipio da substitui\c{c}\~ao. De acordo com esta vis\~ao, os chamados fatores de produ\c{c}\~ao seriam basicamente substitutos, tanto atrav\'es da substitui\c{c}\~ao direta na produ\c{c}\~ao, em fun\c{c}\~ao inversa de seus pre\c{c}os relativos, quanto por meio da substitui\c{c}\~ao indireta de fatores (que ocorre quando os consumidores substituem os produtos relativamente mais caros, por serem mais intensivos em fatores de produ\c{c}\~ao de pre\c{c}o relativamente mais alto, pelos mais baratos). A suposta exist\^encia em desta geral rela\c{c}\~ao inversa entre a quantidade utilizada de cada " fator de produ\c{c}\~ao " e seu pre\c{c}o relativos permitiria, sob condi\c{c}\~oes competitivas, tanto uma tend\^encia de longo prazo \`a plena utiliza\c{c}\~ao das dota\c{c}\~oes de todos os fatores de produ\c{c}\~ao quanto simultaneamente a caracteriza\c{c}\~ao de seus pre\c{c}os relativos como um \'indice de sua escassez relativa. Nesta abordagem, em condi\c{c}\~oes competitivas (isto \'e, com flexibilidade nominal e real de seus pre\c{c}os) todos os fatores de produ\c{c}\~ao que tenham um pre\c{c}o positivo s\~ao necessariamente escassos e tendem a ser plenamente empregados a longo prazo.},
  file = {/home/gpetrini/Zotero/storage/PW4BB6C2/Serrano_Unknown_Observações Sobre as Teorias do Crescimento.pdf},
  journal = {Mimeo},
  keywords = {FICHAR,MIMEO}
}

@article{serrano_politica_nodate,
  title = {Pol\'itica {{Macroecon\^omica}}, Crescimento e Distribui\c{c}\~ao de Renda Na {{Economia Brasileira}} Dos Anos 2000},
  author = {Serrano, Franklin and Summa, Ricardo},
  abstract = {Resumo O presente artigo avalia o crescimento e a distribui\c{c}\~ao da renda na Economia Brasileira nos anos 2000, mostrando que a melhora dos indicadores macroecon\^omicos resulta da intera\c{c}\~ao entre grandes mudan\c{c}as nas condi\c{c}\~oes externas com as quais a economia brasileira se defrontava (desde 2003) e pequenas mudan\c{c}as na orienta\c{c}\~ao da pol\'itica econ\^omica dom\'estica (desde 2005). Al\'em disso, avalia os limites, pelo lado da competitividade externa e da sustentabilidade do Balan\c{c}o de Pagamentos, desse tipo de modelo de crescimento com a manuten\c{c}\~ao do atual sistema de metas de infla\c{c}\~ao. Por fim, prop\~oe algumas alternativas de pol\'itica, mostrando que mais do que meras vis\~oes diferentes sobre o funcionamento da economia, elas dizem tamb\'em respeito a resultados diferentes em termos de distribui\c{c}\~ao funcional da renda. Palavras-chave: Economia Brasileira, Sistema de Metas de Infla\c{c}\~ao; Crescimento e Distribui\c{c}\~ao da Renda. Abstract In the paper we evaluate the growth and income distribution in the Brazilian Economy in the 2000s, showing that the improvement of the macroeconomic indicators results from the interaction between major changes in external conditions with which the Brazilian economy was facing (since 2003) and small changes in orientation domestic economic policy (since 2005). We also access the limits, in terms of external competitiveness and the balance of payments sustainability, of this type of growth regime maintaining the current inflation target system. Finally, we propose some alternative policy, showing that more than mere different views on the functioning of the economy, they also concern the different results in terms of functional income distribution.},
  file = {/home/gpetrini/Zotero/storage/394PY26H/Serrano, Summa_Unknown_Política Macroeconômica, crescimento e distribuição de renda na Economia Brasileira dos anos 2000.pdf},
  keywords = {Brazilian Economy,Growth and Income Distribution,Inflation Target System}
}

