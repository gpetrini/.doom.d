@article{vincens_income_2015,
  title = {Income Inequality, Economic Growth and Stroke Mortality in {{Brazil}}: {{Longitudinal}} and Regional Analysis 2002-2009},
  author = {Vincens, Natalia and Stafstr{\"o}m, Martin},
  year = {2015},
  volume = {10},
