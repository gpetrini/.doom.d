@article{reisTransformacoesRecentesNo2019,
  title = {{Transforma\c{c}\~oes recentes no mercado imobili\'ario na Espanha : auge e crise da produ\c{c}\~ao habitacional sob o ritmo das finan\c{c}as (1998- 2018)}},
  shorttitle = {{Transforma\c{c}\~oes recentes no mercado imobili\'ario na Espanha}},
  author = {Reis, Bruno Pereira},
  year = {2019},
  publisher = {{[s.n.]}},
  abstract = {Resumo: Esta disserta\c{c}\~ao analisa, sob uma perspectiva cr\'itica, as din\^amicas e transforma\c{c}\~oes recentes do mercado imobili\'ario espanhol, com \^enfase no processo de financeiriza\c{c}\~ao do capitalismo. Argumenta-se que a ades\~ao da Espanha \`a Uni\~ao Europeia redefiniu, no contexto europeu, sua posi\c{c}\~ao na divis\~ao internacional do trabalho, a partir da\'i, verifica-se, nas duas \'ultimas d\'ecadas, um conjunto de transforma\c{c}\~oes econ\^omicas e pol\'iticas que repercutiram diretamente em seu processo de urbaniza\c{c}\~ao recente, o que refor\c{c}ou o papel desempenhado pela produ\c{c}\~ao habitacional nas din\^amicas econ\^omicas do pa\'is e produziu um ciclo imobili\'ario sem precedentes. Contudo, este boom imobili\'ario n\~ao se materializou de maneira uniforme pelo territ\'orio espanhol, pelo contr\'ario, a an\'alise da economia regional e urbana do pa\'is indica uma forte concentra\c{c}\~ao espacial deste processo, sobretudo nas principais aglomera\c{c}\~oes urbanas, na costa mediterr\^anea e nos arquip\'elagos. A eclos\~ao da \'ultima crise financeira internacional p\^os fim ao boom imobili\'ario em curso e promoveu uma s\'erie de consequ\^encias na economia, na sociedade e no territ\'orio espanhol, al\'em de uma profunda reorganiza\c{c}\~ao dos mercados financeiro e imobili\'ario do pa\'is. Desde uma perspectiva territorial, o estudo do bairro Arcosur na prov\'incia de Zaragoza representa um dos casos mais emblem\'aticos de obras urbanas interrompidas pela crise, portanto, evidencia aspectos importantes de como as l\'ogicas financeiras e imobili\'arias influenciam o processo de produ\c{c}\~ao do espa\c{c}o urbano na escala local. Considerando os efeitos socioecon\^omicos da crise na Espanha, constatamos uma intensifica\c{c}\~ao dos conflitos sociais, sobretudo porque que as entidades financeiras que receberam dinheiro p\'ublico na gest\~ao da crise tamb\'em foram aquelas que mais tiveram execu\c{c}\~oes hipotecarias em seu favor, configurando, assim, o que Harvey (2003) denomina de acumula\c{c}\~ao por despossess\~ao. Neste contexto, houve uma reorganiza\c{c}\~ao da estrutura e do funcionamento tanto do financeiro como do imobili\'ario no pa\'is, por\'em, o aumento da concentra\c{c}\~ao e centraliza\c{c}\~ao de capital destes setores constituiu um ambiente de neg\'ocios prop\'icio \`a atua\c{c}\~ao de fundos de investimento imobili\'ario nas principais cidades espanholas. Neste sentido, a an\'alise da venda de 1.860 moradias p\'ublicas em Madri ao Grupo Fidere e a Blackstone nos revela tra\c{c}os marcantes das pr\'aticas predat\'orias resultantes das novas articula\c{c}\~oes entre o poder p\'ublico local, o financeiro e imobili\'ario. Esta conjuntura, por sua vez, exigiu por parte da popula\c{c}\~ao uma importante mobiliza\c{c}\~ao social em torno principalmente, da quest\~ao habitacional. Assim, movimentos sociais como a Plataforma dos Afetados pela Hipoteca (PAH) desempenharam um papel extremamente importante no pa\'is alterando a correla\c{c}\~ao de for\c{c}as estabelecida e legitimando a luta pelo direito \`a habita\c{c}\~ao. Em s\'intese, a an\'alise do caso espanhol nos revela como exacerba\c{c}\~ao das finan\c{c}as passa a comandar as mais diversas dimens\~oes da economia e da sociedade, o que faz com que o processo de produ\c{c}\~ao do espa\c{c}o e o acesso a habita\c{c}\~ao estejam submetidos, cada vez mais, aos imperativos do mercado. Deste modo, conclui-se que a implementa\c{c}\~ao de pol\'iticas neoliberais e, consequentemente, o paulatino recuo do Estado de bem-estar social vai na contram\~ao da constru\c{c}\~ao de uma sociedade mais justa e equitativa},
  annotation = {Accepted: 2019-11-28T19:14:36Z},
  file = {/home/gpetrini/Zotero/storage/85U8N9MK/Reis - 2019 - Transformações recentes no mercado imobiliário na .pdf;/home/gpetrini/Zotero/storage/I6H77P8E/335576.html},
  language = {Portugu\^es}
}

