  file = {/home/gpetrini/Zotero/storage/DGQM9HAQ/Pereira - 2019 - Preços imobiliários e ciclos econômicos nos anos 2.pdf;/home/gpetrini/Zotero/storage/EEXUZP5D/335423.html},
