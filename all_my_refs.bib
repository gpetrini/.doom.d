  file = {/home/gpetrini/Zotero/storage/76PKDD2K/Pariboni - 2016 - Household Consumer Debt, Endogenous Money and Grow.pdf;/home/gpetrini/Zotero/storage/6989XPVP/papers.html},
  keywords = {economic growth,effective demand,supermultiplier},
  language = {en},
  number = {ID 3100768},
  type = {{{SSRN Scholarly Paper}}}
}

